{
    "name": "loopWorkflow",
    "workflows": [
        {
            "name": "loopWorkflow",
            "branches": [
                {
                    "conditions": [
                        {
                            "action_name": "regMatch",
                            "app_name": "HelloWorldBounded",
                            "arguments": [
                                {
                                    "name": "regex",
                                    "value": "1|2|3|4"
                                }
                            ],
                            "transforms": []
                        }
                    ],
                    "destination_id": -1,
                    "source_id": -1,
                    "status": "Success"
                },
                {
                    "conditions": [
                        {
                            "action_name": "regMatch",
                            "app_name": "HelloWorldBounded",
                            "arguments": [
                                {
                                    "name": "regex",
                                    "value": "5"
                                }
                            ],
                            "transforms": []
                        }
                    ],
                    "destination_id": -2,
                    "source_id": -1,
                    "status": "Success"
                }
            ],
            "start": -1,
            "actions": [
                {
                    "action_name": "repeatBackToMe",
                    "app_name": "HelloWorldBounded",
                    "arguments": [
                        {
                            "name": "call",
                            "value": "{{outputFrom(actions, -1)}}"
                        }
                    ],
                    "name": "1",
                    "templated": true,
                    "id": -2,
                    "device_id": 1
                },
                {
                    "action_name": "returnPlusOne",
                    "app_name": "HelloWorldBounded",
                    "arguments": [
                        {
                            "name": "number",
                            "value": "{%- if actions | length > 0 -%}\n                        {%- set x = outputFrom(actions, -1) -%}\n                    {%- endif -%}\n\n                    {%- if x is not none and x is defined -%}\n                        {{ x }}\n                    {%- else -%}\n                        1\n                    {%- endif -%}\n                "
                        }
                    ],
                    "name": "start",
                    "templated": true,
                    "id": -1,
                    "device_id": 1
                }
            ]
        }
    ]
}